  "scripts": {
    "start": "SERVER_PORT=3030 npm-run-all --parallel start-client start-server",
    "start": "SERVER_PORT=3030 npm-run-all --parallel start-client start-server-dev",
    "start-client": "cd client && npm start",
    "start-server": "cd server && npm start",
